<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}UserFilter Demo{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <ul>
            <li><a href="{{ url('all') }}">All users</a></li>
            <li>Filter examples: {% for i in range(1, 3) %}<a href="{{ url('filter', {id: i}) }}">{{ i }}</a>, {% endfor %}</li>
        </ul>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
